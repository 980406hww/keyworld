<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.keymanager.ckadmin.dao.GroupDao">

    <update id="updateGroupOperationCombineUuid">
        UPDATE t_group
        SET fUpdateTime = NOW(), fOperationCombineUuid = #{operationCombineUuid}
        WHERE fGroupName = #{groupName}
    </update>

    <delete id="deleteByGroupName">
        DELETE FROM t_group
        WHERE fGroupName = #{groupName}
    </delete>
</mapper>