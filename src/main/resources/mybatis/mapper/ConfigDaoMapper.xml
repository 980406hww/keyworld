<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.keymanager.monitoring.dao.ConfigsDao">
    <select id="getConfig" resultType="com.keymanager.monitoring.entity.Config">
        SELECT c.fConfigType, c.fKey, c.fValue FROM t_config c WHERE c.fConfigType = #{configType} AND c.fKey = #{key}
    </select>
</mapper>