<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.keymanager.monitoring.dao.ServerAddressDao">
    <sql id="colID">
    </sql>

    <select id="searchServerAddressList" resultType="com.keymanager.monitoring.entity.ServerAddress">
        SELECT
          sa.fUuid AS 'uuid',
          sa.fServerAddress AS 'serverAddress'
        FROM t_server_address_info sa
        WHERE 1=1
        <if test="serverAddressCriteria.serverAddress != null and serverAddressCriteria.serverAddress != ''">
          AND sa.fServerAddress like '%${serverAddressCriteria.serverAddress}%'
        </if>

    </select>

</mapper>